    "@storybook/manager-webpack5": "^6.4.18",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.3",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.5"
